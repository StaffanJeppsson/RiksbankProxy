{
  "name": "riksbank-api",
  "version": "1.0.0",
  "description": "REST API for Riksbank monetary policy data",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "build": "tsc",
